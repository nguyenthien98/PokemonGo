<!DOCTYPE html>
<html lang="en">

<head>
	<title>Login V3</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!--===============================================================================================-->
	<link rel="icon" type="image/png" href="images/icons/favicon.ico" />
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/bootstrap/css/bootstrap.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="fonts/iconic/css/material-design-iconic-font.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/css-hamburgers/hamburgers.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/animsition/css/animsition.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/select2/select2.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/daterangepicker/daterangepicker.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="css/util.css">
	<link rel="stylesheet" type="text/css" href="css/main.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU"
	 crossorigin="anonymous">
	<link rel="stylesheet" href="css/font-awesome.min.css">
	<script src="https://code.jquery.com/jquery-1.11.1.min.js"></script>
	<script src="js/login.js" type="text/javascript"></script>

</head>

<body>
	
	<div class="limiter">
		<div class="container-login100" style="background-image: url('images/sg.jpg');">
			<div class="col-lg-4">
				<div style="margin-top: 3%">
					<% for(let ban of banner){ %>
						<% if(ban.trang== 1 && ban.vitri==1) { %>
							<a class="bannerZ" href="" data-url="<%= ban.link %>" id="<%= ban._id%>" >
								<img value="Count"  style="width: 100%; margin-bottom: 4%; padding-right: 2% " src="<%= ban.imageURL%>"  />
							</a>
					<% }} %>
				</div>

				<!-- <div>
					<a href="https://kimdungquanhiep.vn/"><img style="width: 100%;margin-top: -1%; padding-right: 2% "src="images/banner/banner-game-pokemon-15.jpg"></a>
				</div>
				<div>
					<a href="http://kiemvu.360game.vn/"><img src="images/banner/banner-game-pokemon-10.jpg" style="width: 98%; margin-top: 5%"></a>
				</div>
				<div>
						<a href="http://2game.vn/game/kiem-vuong-chi-mong"><img src="images/banner/banner-game-pokemon-14.jpg" style=" width: 98%; margin-top: 5%"></a>
				</div> -->
			</div>

			<div class="wrap-login100 col-lg-4">
				<form class="login100-form validate-form">
					<!-- <span class="login100-form-logo">
						<i class="zmdi zmdi-landscape"></i>
					</span> -->
					<img style="padding-left: 19%" src="images/Poke.png">

					<span class="login100-form-title p-b-34 p-t-27">
						Log in
					</span>

					<div class="wrap-input100 validate-input" data-validate="Enter uname">
						<input class="input100" type="text" name="uname" placeholder="Username" id="uname">
						<span class="focus-input100" data-placeholder="&#xf207;"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Enter pass">
						<input class="input100" type="password" name="upass" placeholder="Password" id="upass">
						<span class="focus-input100" data-placeholder="&#xf191;"></span>
					</div>

					<div class="contact100-form-checkbox">
						<input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
						<label class="label-checkbox100" for="ckb1" style="display: inline-block;margin-left: 9%">
							Remember me
						</label>
						<i style="display: inline-block;color: white;padding-left: 27%" class="fas fa-user-plus"></i>
						<a href="/signup">
							<p style="display: inline-block;color: white;padding-left: 1%">Sign Up</p>
						</a>
					</div>
					<div>

					</div>
					<div class="container-login100-form-btn" style="padding-top: 3%">
						<button class="login100-form-btn" id="loginForm">
							Login
						</button>
					</div>

					<div class="text-center" style="padding-top: 19%">
						<a class="txt1" href="#">
							Forgot Password?
						</a>
					</div>
				</form>
			</div>
			<div class="col-lg-4">
					<% for(let ban of banner){ %>
						<% if(ban.trang== 1 && ban.vitri==2) { %>
							<a  class="bannerZ" href="" data-url="<%= ban.link %>" id="<%= ban._id%>">
								<img style="width: 89%; margin-left: 2%" src="<%= ban.imageURL%>" />
							</a>
					<% }} %>
				<!-- <a href="http://marvel-asia.com/avengers4/vn?int_cmp=lp:24h:banner:avengers4-endgame&utm_source=24h&utm_medium=banner&utm_campaign=avengers4-endgame"><img
					 src="images/banner/banner-game-pokemon-4.jpg" style="width: 89%; margin-left: 2%"></a> -->
			</div>
		</div>
	</div>


	<div id="dropDownSelect1"></div>

	<!--===============================================================================================-->
	<script src="vendor/jquery/jquery-3.2.1.min.js"></script>
	<!--===============================================================================================-->
	<script src="vendor/animsition/js/animsition.min.js"></script>
	<!--===============================================================================================-->
	<script src="vendor/bootstrap/js/popper.js"></script>
	<script src="vendor/bootstrap/js/bootstrap.min.js"></script>
	<!--===============================================================================================-->
	<script src="vendor/select2/select2.min.js"></script>
	<!--===============================================================================================-->
	<script src="vendor/daterangepicker/moment.min.js"></script>
	<script src="vendor/daterangepicker/daterangepicker.js"></script>
	<!--===============================================================================================-->
	<script src="vendor/countdowntime/countdowntime.js"></script>
	<!--===============================================================================================-->
	<script src="js/main.js"></script>
	<script src="js/getBanner.js"></script>
	<script src="js/click_banner.js"></script>
	
</body>

</html>